
import personajes.*

program PepitaGame {
	game.title("pepita")
	game.width(15)
	game.height(24)
	game.cellSize(50)
	tablero.agregarPersonaje(monje)
	tablero.agregarPersonaje(monje2)
	tablero.agregarPersonaje(new Torre(position = game.at(7,21)))
	tablero.agregarPersonaje(new Torre(position = game.at(3,17)))
	tablero.agregarPersonaje(new Torre(position = game.at(11,17)))
	tablero.visualizar()
	game.addVisualCharacter(marco)
	game.boardGround("map.png")
	game.start()
}
